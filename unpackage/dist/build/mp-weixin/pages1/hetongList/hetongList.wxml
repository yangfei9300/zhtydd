<view><view class="colonn w-750 h-1500"><block wx:for="{{htList}}" wx:for-item="item" wx:for-index="index"><view data-event-opts="{{[['tap',[['toinfo',['$event']]]]]}}" class="colonn background1 br-20 m-w-710 m-all-20 b-all-20" catchtap="__e"><view class="roww border_bottom p-all-20 fs-30"><view>{{item.id}}</view><view class="allline"></view><view class="roww"><block wx:if="{{item.status=='0'}}"><block>未初始化</block></block><block wx:if="{{item.status=='1'||item.status=='2'}}"><block>已生成文件</block></block><block wx:if="{{item.status=='3'}}"><block>业务员已签字</block></block><block wx:if="{{item.status=='4'}}"><block>企业已签字</block></block></view></view><view class="h-10"></view><view class="roww fs-30" style="padding:10rpx 20rpx;"><view>{{"企业："+item.customerName}}</view></view><view class="roww p-all-20 fs-30" style="padding:10rpx 20rpx;"><view>{{"签订金额：￥"+item.totalRmb}}</view></view><view class="roww p-all-20 fs-30" style="padding:10rpx 20rpx;"><view>{{"所属业务员："+item.params.nickName}}</view></view><view class="roww"><view class="allline"></view><block wx:if="{{identityType==2}}"><block><block wx:if="{{item.status==0}}"><view data-event-opts="{{[['tap',[['clickHt',['$0'],[[['htList','',index]]]]]]]}}" class="chakaninfo" catchtap="__e">生成合同</view></block><block wx:if="{{item.status==1||item.status==2}}"><view data-event-opts="{{[['tap',[['toQySign',['$0'],[[['htList','',index]]]]]]]}}" class="chakaninfo" catchtap="__e">签字</view></block></block></block><block wx:if="{{identityType==1}}"><block><block wx:if="{{item.status==3}}"><view data-event-opts="{{[['tap',[['toQySign',['$0'],[[['htList','',index]]]]]]]}}" class="chakaninfo" catchtap="__e">签字</view></block></block></block><view data-event-opts="{{[['tap',[['toInfo',['$0'],[[['htList','',index]]]]]]]}}" class="chakaninfo" catchtap="__e">查看详情</view><view class="w-20"></view></view><view class="h-15"></view></view></block></view></view>